module modulo_5counter(reset, w, z, nexState);
	input reset,w;
	output z ;
	output [2:0] nexState;
	reg [2:0] currState;

	currState = 3'b000;
	
	if(currState != 3'b100) begin
		assign z = 1'b0;
	end else begin
		assign z = 1'b1;
	end
	
	if(w == 1'b1)begin
		assign z = 1'b0;
	end else begin
		assign z = 1'b1;
	end
	
endmodule
